{
  "name": "@dinstack/web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 4000",
    "dev:extension": "pnpm run dev",
    "dev:web": "pnpm run dev",
    "build": "tsc --build && tsc-alias",
    "ui:add": "pnpm dlx shadcn-ui@latest add -y",
    "preview": "env-cmd -f .env.local vite build && vite preview --port 4000",
    "typecheck": "tsc --noEmit",
    "deploy:preview": "env-cmd -f .env.preview vite build && wrangler pages deploy --project-name=dinstack-web-preview --branch=main ./dist",
    "deploy:production": "env-cmd -f .env.production vite build && wrangler pages deploy --project-name=dinstack-web-production --branch=main ./dist"
  },
  "dependencies": {
    "@api": "workspace:@dinstack/api@^",
